:Parent a owl:Class ;
    rdfs:subClassOf :Person .

:Writer a owl:Class ;
    rdfs:subClassOf :Person .

:Study a owl:Class ;
    rdfs:subClassOf :ResearchPaper .

:Research a owl:Class ;
    rdfs:subClassOf :ResearchPaper .

:talksAbout a owl:ObjectProperty ;
    rdfs:domain :Parent ;
    rdfs:range :Thing .

:wrote a owl:ObjectProperty ;
    rdfs:domain :Writer ;
    rdfs:range :Thing .

:continues a owl:ObjectProperty ;
    rdfs:domain :Writer ;
    rdfs:range :Thing .

:summedItUpAs a owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :Thing .

:suggests a owl:ObjectProperty ;
    rdfs:domain :Research ;
    rdfs:range :Thing .

:says a owl:ObjectProperty ;
    rdfs:domain :Study ;
    rdfs:range :Thing .