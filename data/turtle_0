:Country a owl:Class .

:MilitaryVehicle a owl:Class ;
    rdfs:subClassOf :Hardware .

:Government a owl:Class .

:Document a owl:Class .

:Brigade a owl:Class .

:Battle a owl:Class .

:Server a owl:Class ;
    rdfs:subClassOf :Person .

:provides a owl:ObjectProperty ;
    rdfs:domain :Country ;
    rdfs:range :Country .

:sends a owl:ObjectProperty ;
    rdfs:domain :Country ;
    rdfs:range :MilitaryVehicle .

:announces a owl:ObjectProperty ;
    rdfs:domain :Government ;
    rdfs:range :Battle .

:occurs a owl:ObjectProperty ;
    rdfs:domain :Battle ;
    rdfs:range :TimePeriod .

:delivers a owl:ObjectProperty ;
    rdfs:domain :Country ;
    rdfs:range :MilitaryVehicle .

:receives a owl:ObjectProperty ;
    rdfs:domain :Brigade ;
    rdfs:range :MilitaryVehicle .

:indicates a owl:ObjectProperty ;
    rdfs:domain :Document ;
    rdfs:range :Fact .

:crosses a owl:ObjectProperty ;
    rdfs:domain :Soldier ;
    rdfs:range :Territory .

:credits a owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :MilitaryVehicle .

:refuses a owl:ObjectProperty ;
    rdfs:domain :Server ;
    rdfs:range :Action .
