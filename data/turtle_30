:Parenting a owl:Class .

:Author a owl:Class ;
    rdfs:subClassOf :Person .

:Book a owl:Class ;
    rdfs:subClassOf :Product .

:SocialMediaPlatform a owl:Class .

:Community a owl:Class .

:authorOf a owl:ObjectProperty ;
    rdfs:domain :Author ;
    rdfs:range :Book .

:tookTo a owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :SocialMediaPlatform .

:discusses a owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :Community .

:CarolineLeaf a :Author .

:HowToHelpYourChildCleanUpTheirMentalMess a :Book .

:CarolineLeaf :authorOf :HowToHelpYourChildCleanUpTheirMentalMess .

:TikTok a :SocialMediaPlatform .

:Kaaviya a :Person .

:Kaaviya :tookTo :TikTok .

:DesiCommunity a :Community .

:Kaaviya :discusses :DesiCommunity .