:Murder a owl:Class .

:SerialKiller a owl:Class ;
    rdfs:subClassOf :Person .

:Lawyer a owl:Class ;
    rdfs:subClassOf :Person .

:Neighbor a owl:Class ;
    rdfs:subClassOf :Person .

:Charged a owl:Class ;
    rdfs:subClassOf :Murder .

:Denies a owl:Class ;
    rdfs:subClassOf :Murder .

:chargedWith a owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :Murder .

:denies a owl:ObjectProperty ;
    rdfs:domain :Lawyer ;
    rdfs:range :Murder .

:hasNeighbor a owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :Neighbor .

:visits a owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:devotedTo a owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :Person .

:tinkersWith a owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :Product .

:represents a owl:ObjectProperty ;
    rdfs:domain :Lawyer ;
    rdfs:range :Person .

:winsElection a owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :Person .